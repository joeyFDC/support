{
    "name": "fdc/support",
    "description": "Helpful things for Laravel",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Joey Harris",
            "email": "jharris@fulfillment.com",
            "homepage": "https://github.com/joeyFDC"
        }
    ],
    "homepage": "https://github.com/joeyFDC/fdc-support",
    "keywords": [
        "laravel",
        "helpers",
        "fdc"
    ],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "orchestra/testbench": "~5|~6",
        "phpunit/phpunit": "~9.0"
    },
    "autoload": {
        "psr-4": {
            "FDC\\Support\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "FDC\\Support\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FDC\\Support\\SupportServiceProvider"
            ],
            "aliases": {
                "Filter": "FDC\\Support\\Facades\\Filter"
            }
        }
    }
}
